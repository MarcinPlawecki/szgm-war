<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
 
	<application>
    	<el-resolver>
    		org.springframework.web.jsf.el.SpringBeanFacesELResolver
    	</el-resolver>
     	<locale-config>
     		<default-locale>pl</default-locale>
     	</locale-config>
	  	<message-bundle>
	  		szgm.core.Messages
	  	</message-bundle>
	</application>
 	
	<managed-bean>
		<managed-bean-name>vat</managed-bean-name>
		<managed-bean-class>szgm.vat.VatBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{vatBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jednostka</managed-bean-name>
		<managed-bean-class>szgm.jednostka.JednostkaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{jednostkaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>waluta</managed-bean-name>
		<managed-bean-class>szgm.waluta.WalutaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{walutaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>grupa</managed-bean-name>
		<managed-bean-class>szgm.grupa.GrupaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{grupaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sklep</managed-bean-name>
		<managed-bean-class>szgm.sklep.SklepBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{sklepBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>uzytkownik</managed-bean-name>
		<managed-bean-class>szgm.uzytkownik.UzytkownikBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{uzytkownikBo}</value>
		</managed-property>
		<managed-property>
			<property-name>passwordEncoder</property-name>
			<value>#{passwordEncoder}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rola</managed-bean-name>
		<managed-bean-class>szgm.rola.RolaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>rolaBo</property-name>
			<value>#{rolaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>typDokumentu</managed-bean-name>
		<managed-bean-class>szgm.typDokumentu.TypDokumentuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{typDokumentuBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>konfiguracja</managed-bean-name>
		<managed-bean-class>szgm.konfiguracja.KonfiguracjaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{konfiguracjaBo}</value>
		</managed-property>
		<managed-property>
			<property-name>konfBo</property-name>
			<value>#{konfiguracjaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>towar</managed-bean-name>
		<managed-bean-class>szgm.towar.TowarBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>towarBo</property-name>
			<value>#{towarBoProxy}</value>
		</managed-property>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{towarBoProxy}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>kontrahent</managed-bean-name>
		<managed-bean-class>szgm.kontrahent.KontrahentBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{kontrahentBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dokPP</managed-bean-name>
		<managed-bean-class>szgm.dokp.poz.DokPPBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dokPPBo</property-name>
			<value>#{dokPPBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dokPN</managed-bean-name>
		<managed-bean-class>szgm.dokp.nagl.DokPNBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{dokPNBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>prawo</managed-bean-name>
		<managed-bean-class>szgm.prawo.PrawoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bo</property-name>
			<value>#{prawoBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>raport</managed-bean-name>
		<managed-bean-class>szgm.raport.RaportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dokWBo</property-name>
			<value>#{dokWNBo}</value>
		</managed-property>
		<managed-property>
			<property-name>dokPBo</property-name>
			<value>#{dokPNBo}</value>
		</managed-property>
	</managed-bean>
	
	
	<navigation-rule>
    	<from-view-id>/towary/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/towary/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	<navigation-rule>
    	<from-view-id>/ustawienia/konfiguracja/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/ustawienia/konfiguracja/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	<navigation-rule>
    	<from-view-id>/kontrahenci/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/kontrahenci/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	<navigation-rule>
    	<from-view-id>/ustawienia/uzytkownik/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/ustawienia/uzytkownik/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	<navigation-rule>
    	<from-view-id>/ustawienia/rola/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/ustawienia/rola/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
        <display-name>common/login.xhtml</display-name>
        <from-view-id>/common/login.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{loginMgmtBean.login}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{loginMgmtBean.login}</from-action>
            <from-outcome>error</from-outcome>
            <to-view-id>/common/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <display-name>statystyki/sprz_grupy.xhtml</display-name>
        <from-view-id>/statystyki/sprz_grupy.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{raport.generujSprzedaz}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/statystyki/sprz_grupy_chart.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>statystyki/marza_grupy.xhtml</display-name>
        <from-view-id>/statystyki/marza_grupy.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{raport.generujMarze}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/statystyki/marza_grupy_chart.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
	<converter>
    	<converter-id>vatConverter</converter-id>
    	<converter-class>szgm.vat.VatConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>walutaConverter</converter-id>
    	<converter-class>szgm.waluta.WalutaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>grupaConverter</converter-id>
    	<converter-class>szgm.grupa.GrupaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>jednostkaConverter</converter-id>
    	<converter-class>szgm.jednostka.JednostkaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>sklepConverter</converter-id>
    	<converter-class>szgm.sklep.SklepConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>uzytkownikConverter</converter-id>
    	<converter-class>szgm.uzytkownik.UzytkownikConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>rolaConverter</converter-id>
    	<converter-class>szgm.rola.RolaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>typDokumentuConverter</converter-id>
    	<converter-class>szgm.typDokumentu.TypDokumentuConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>konfiguracjaConverter</converter-id>
    	<converter-class>szgm.konfiguracja.KonfiguracjaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>kontrahentConverter</converter-id>
    	<converter-class>szgm.kontrahent.KontrahentConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>dokPNConverter</converter-id>
    	<converter-class>szgm.dokp.nagl.DokPNConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>dokPPConverter</converter-id>
    	<converter-class>szgm.dokp.poz.DokPPConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>towarConverter</converter-id>
    	<converter-class>szgm.towar.TowarConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>prawoConverter</converter-id>
    	<converter-class>szgm.prawo.PrawoConverter</converter-class>
	</converter>
	
</faces-config>