<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
 
	<application>
    	<el-resolver>
    		org.springframework.web.jsf.el.SpringBeanFacesELResolver
    	</el-resolver>
  	</application>
 	
	<managed-bean>
		<managed-bean-name>vat</managed-bean-name>
		<managed-bean-class>szgm.vat.VatBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>vatBo</property-name>
			<value>#{vatBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jednostka</managed-bean-name>
		<managed-bean-class>szgm.jednostka.JednostkaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>jednostkaBo</property-name>
			<value>#{jednostkaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>waluta</managed-bean-name>
		<managed-bean-class>szgm.waluta.WalutaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>walutaBo</property-name>
			<value>#{walutaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>grupa</managed-bean-name>
		<managed-bean-class>szgm.grupa.GrupaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>grupaBo</property-name>
			<value>#{grupaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sklep</managed-bean-name>
		<managed-bean-class>szgm.sklep.SklepBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>sklepBo</property-name>
			<value>#{sklepBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>uzytkownik</managed-bean-name>
		<managed-bean-class>szgm.uzytkownik.UzytkownikBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>uzytkownikBo</property-name>
			<value>#{uzytkownikBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rola</managed-bean-name>
		<managed-bean-class>szgm.rola.RolaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>rolaBo</property-name>
			<value>#{rolaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>typDokumentu</managed-bean-name>
		<managed-bean-class>szgm.typDokumentu.TypDokumentuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>typDokumentuBo</property-name>
			<value>#{typDokumentuBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>konfiguracja</managed-bean-name>
		<managed-bean-class>szgm.konfiguracja.KonfiguracjaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>konfiguracjaBo</property-name>
			<value>#{konfiguracjaBo}</value>
		</managed-property>
	</managed-bean>
	
	<navigation-rule>
    	<from-view-id>/towary/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/towary/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	<navigation-rule>
    	<from-view-id>/konfiguracja/add.xhtml</from-view-id>
    	<navigation-case>
        	<from-outcome>success</from-outcome>
        	<to-view-id>/konfiguracja/list.xhtml</to-view-id>
    	</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
        <display-name>common/login.xhtml</display-name>
        <from-view-id>/common/login.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{loginMgmtBean.login}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <display-name>common/login.xhtml</display-name>
        <from-view-id>/common/login.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{loginMgmtBean.login}</from-action>
            <from-outcome>error</from-outcome>
            <to-view-id>/common/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
	<converter>
    	<converter-id>vatConverter</converter-id>
    	<converter-class>szgm.vat.VatConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>walutaConverter</converter-id>
    	<converter-class>szgm.waluta.WalutaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>grupaConverter</converter-id>
    	<converter-class>szgm.grupa.GrupaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>jednostkaConverter</converter-id>
    	<converter-class>szgm.jednostka.JednostkaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>sklepConverter</converter-id>
    	<converter-class>szgm.sklep.SklepConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>uzytkownikConverter</converter-id>
    	<converter-class>szgm.uzytkownik.UzytkownikConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>rolaConverter</converter-id>
    	<converter-class>szgm.rola.RolaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>typDokumentuConverter</converter-id>
    	<converter-class>szgm.typDokumentu.TypDokumentuConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>konfiguracjaConverter</converter-id>
    	<converter-class>szgm.konfiguracja.KonfiguracjaConverter</converter-class>
	</converter>
	
</faces-config>