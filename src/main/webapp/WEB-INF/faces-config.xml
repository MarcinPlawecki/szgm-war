<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
 
	<application>
    	<el-resolver>
    		org.springframework.web.jsf.el.SpringBeanFacesELResolver
    	</el-resolver>
  	</application>
 	
	<managed-bean>
		<managed-bean-name>towar</managed-bean-name>
		<managed-bean-class>szgm.TowarBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>towarBo</property-name>
			<value>#{towarBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>vat</managed-bean-name>
		<managed-bean-class>szgm.VatBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>vatBo</property-name>
			<value>#{vatBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jednostka</managed-bean-name>
		<managed-bean-class>szgm.JednostkaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>jednostkaBo</property-name>
			<value>#{jednostkaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>waluta</managed-bean-name>
		<managed-bean-class>szgm.WalutaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>walutaBo</property-name>
			<value>#{walutaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>grupa</managed-bean-name>
		<managed-bean-class>szgm.GrupaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>grupaBo</property-name>
			<value>#{grupaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sklep</managed-bean-name>
		<managed-bean-class>szgm.SklepBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>sklepBo</property-name>
			<value>#{sklepBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>uzytkownik</managed-bean-name>
		<managed-bean-class>szgm.UzytkownikBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>uzytkownikBo</property-name>
			<value>#{uzytkownikBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rola</managed-bean-name>
		<managed-bean-class>szgm.RolaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>rolaBo</property-name>
			<value>#{rolaBo}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>typDokumentu</managed-bean-name>
		<managed-bean-class>szgm.TypDokumentuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>typDokumentuBo</property-name>
			<value>#{typDokumentuBo}</value>
		</managed-property>
	</managed-bean>
	
	
	<navigation-rule>
    	<from-view-id>/towary/dodaj.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/towary/index.xhtml</to-view-id>
    </navigation-case>
	</navigation-rule>
	
	
	<converter>
    	<converter-id>vatConverter</converter-id>
    	<converter-class>szgm.converters.VatConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>walutaConverter</converter-id>
    	<converter-class>szgm.converters.WalutaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>grupaConverter</converter-id>
    	<converter-class>szgm.converters.GrupaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>jednostkaConverter</converter-id>
    	<converter-class>szgm.converters.JednostkaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>sklepConverter</converter-id>
    	<converter-class>szgm.converters.SklepConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>uzytkownikConverter</converter-id>
    	<converter-class>szgm.converters.UzytkownikConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>rolaConverter</converter-id>
    	<converter-class>szgm.converters.RolaConverter</converter-class>
	</converter>
	<converter>
    	<converter-id>typDokumentuConverter</converter-id>
    	<converter-class>szgm.converters.TypDokumentuConverter</converter-class>
	</converter>
	
	
</faces-config>